{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "api-key-manager",
  "version": "0.1.0",
  "identifier": "com.hongyu-li.api-key-manager",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:61820",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "API Key Manager - Floating Toolbar",
        "width": 460,
        "height": 120,
        "decorations": false,
        "skipTaskbar": false,
        "transparent": true,
        "hiddenTitle": true,
        "visible": true,
        "alwaysOnTop": true,
        "center": true,
        "shadow": false,
        "theme": "Light",
        "acceptFirstMouse": false,
        "dragDropEnabled": false,
        "resizable": false,
        "minimizable": true,
        "maximizable": false,
        "closable": true,
        "fullscreen": false,
        "titleBarStyle": "Overlay",
        "visibleOnAllWorkspaces": false,
        "contentProtected": false
      }
    ],

    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/64x64.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/256x256.png",
      "icons/512x512.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}